{
  "name": "anki-canvas",
  "version": "0.0.1",
  "description": "JavaScript widget providing touch enabled canvas for drawing in AnkiDroid",
  "main": "index.js",
  "repository": "https://github.com/pigoz/anki-canvas",
  "author": "pigoz",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "chalk": "^2.4.1",
    "jest": "^23.5.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "test": "jest",
    "build": "parcel build src/jplookup.ts src/sub2srs.ts",
    "clean": "rm -rf dist",
    "dist": "cp dist/jplookup.js ~/dev/dotfiles/mpv/scripts/jplookup.js && cp dist/sub2srs.js ~/dev/dotfiles/mpv/scripts/sub2srs.js"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true,
        "mapCoverage": true
      }
    },
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "/test/.*\\.test\\.ts$"
  }
}
